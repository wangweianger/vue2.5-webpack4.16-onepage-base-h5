{
  "name": "webpack4-vue2",
  "version": "0.0.1",
  "description": "zane project",
  "main": "main.js",
  "scripts": {
    "dev": "cross-env PROT=8000 NODE_ENV=development node build/webpack.dev.config.js",
    "test": "cross-env NODE_ENV=production webpack --progress --hide-modules --colors --config build/webpack.test.config.js",
    "test:dll": "cross-env BUILD_TYPE=test webpack --progress --config build/webpack.dll.config.js",
    "build": "cross-env NODE_ENV=production webpack --progress --colors --config build/webpack.product.config.js",
    "build:dll": "cross-env BUILD_TYPE=build webpack --progress --config build/webpack.dll.config.js",
    "test-build": "npm run build && node ./build/http.push.tar.gz.js"
  },
  "author": "wanwei",
  "license": "MIT",
  "engines": {
    "node": ">=7.0",
    "npm": ">=3.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "vue": "^2.5.17",
    "vue-loader": "^15.3.0",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "accounting": "^0.4.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "^4.9.3",
    "open-browser-webpack-plugin": "^0.0.5",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^2.1.6",
    "progress-bar-webpack-plugin": "^1.11.0",
    "sass-loader": "^7.1.0",
    "speed-measure-webpack-plugin": "^1.2.2",
    "string-replace-loader": "^2.1.1",
    "style-loader": "^0.22.0",
    "url-loader": "^1.0.1",
    "vue-router-lazy-replace-loader": "0.0.1",
    "vue-style-loader": "^4.1.1",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "webpack-dll-loader": "0.0.1",
    "webpack-merge": "^4.1.4",
    "webpack-parallel-uglify-plugin": "^1.1.0"
  }
}
